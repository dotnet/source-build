From: adaggarwal <aditya.aggarwal@microsoft.com>
Date: Fri, 7 Feb 2020 05:52:17 +0000
Subject: [PATCH 15/15] change-runtimeID-to-Sourcebuild-RID

---
 src/Framework/src/Microsoft.AspNetCore.App.Runtime.csproj | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/src/Framework/src/Microsoft.AspNetCore.App.Runtime.csproj b/src/Framework/src/Microsoft.AspNetCore.App.Runtime.csproj
index 39f78a11b0..fe6a4f292c 100644
--- a/src/Framework/src/Microsoft.AspNetCore.App.Runtime.csproj
+++ b/src/Framework/src/Microsoft.AspNetCore.App.Runtime.csproj
@@ -3,7 +3,7 @@
 
   <PropertyGroup>
     <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
-    <RuntimeIdentifier>$(TargetRuntimeIdentifier)</RuntimeIdentifier>
+    <RuntimeIdentifier>$(SourceBuildRuntimeIdentifier)</RuntimeIdentifier>
     <!-- Even though RuntimeIdentifier is set, shared framework projects are not self-contained projects -->
     <SelfContained>false</SelfContained>
     <PackageId>$(MSBuildProjectName).$(RuntimeIdentifier)</PackageId>
