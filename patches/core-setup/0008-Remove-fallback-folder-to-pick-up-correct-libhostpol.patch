@@ -26,8 +26,6 @@
     <RestoreAllBuildRids>false</RestoreAllBuildRids>
     <!-- Ensure only this repo's NuGet.config file is used. -->
     <RestoreConfigFile>$(ProjectDir)NuGet.config</RestoreConfigFile>
