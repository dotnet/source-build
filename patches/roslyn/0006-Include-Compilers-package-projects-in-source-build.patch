+    <ExcludeFromSourceBuild>false</ExcludeFromSourceBuild>
     <NuspecPackageId>Microsoft.Net.Compilers.Toolset</NuspecPackageId>
     <IncludeBuildOutput>false</IncludeBuildOutput>
