@@ -7,6 +7,7 @@
     <OptimizationDataSupported Condition="'$(TargetOS)' == 'Linux' And '$(BuildArch)' == 'x64'">True</OptimizationDataSupported>
     <RuntimeIdentifiers>win7-x64;win7-x86;linux-x64</RuntimeIdentifiers>
