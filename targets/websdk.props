<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebSdkVersion>1.0.0-alpha-20170130-3-281</WebSdkVersion>
    <CoreSdkVersion>1.1.0-alpha-20170228-1</CoreSdkVersion>
    <ProjectDirectory Condition="'$(PathToRepo)' != ''">$(PathToRepo)</ProjectDirectory>
    
    <DotnetToolsetInstallOverride>DOTNET_INSTALL_DIR=$(PreBuiltDotnetCliDir) </DotnetToolsetInstallOverride>
    <OverridesForCommands>$(PackagesOverride) $(DotnetToolsetInstallOverride) </OverridesForCommands>
    <EnvironmentSetPrefix Condition="'$(OS)' == 'Windows_NT'">(set WebSdkVersion=$(WebSdkVersion)) &amp; (set CoreSdkVersion=$(CoreSdkVersion)) &amp;</EnvironmentSetPrefix>
    <EnvironmentSetPrefix Condition="'$(OS)' != 'Windows_NT'">WebSdkVersion=$(WebSdkVersion) CoreSdkVersion=$(CoreSdkVersion)</EnvironmentSetPrefix>
    <SyncCommand Condition="'$(OS)' != 'Windows_NT'" >$(OverridesForCommands) SKIP_BUILD=true $(ProjectDirectory)/build-core$(ShellExtension)</SyncCommand>
    <BuildCommand>$(EnvironmentSetPrefix) $(OverridesForCommands) SKIP_BUILD=false $(ProjectDirectory)/build-core$(ShellExtension)</BuildCommand>
    <PackagesOutput>$(ProjectDirectory)/bin/Release/NuGet/</PackagesOutput>
  </PropertyGroup>
</Project>
