          DependsOnTargets="GetTarballDirProps">
    <ItemGroup>
      <KnownExtraPrebuiltFile Include="
        $(TarballPrebuiltPackageDir)dotnet-dev-certs.3.1.6-servicing.20326.1.nupkg;
