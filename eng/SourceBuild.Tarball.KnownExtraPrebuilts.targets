        $(TarballPrebuiltPackageDir)nuget.librarymodel.5.4.0.nupkg;
        $(TarballPrebuiltPackageDir)nuget.packaging.5.4.0.nupkg;
        $(TarballPrebuiltPackageDir)nuget.projectmodel.5.4.0.nupkg;
        $(TarballPrebuiltPackageDir)nuget.protocol.5.4.0.nupkg;
        $(TarballPrebuiltPackageDir)nuget.versioning.5.4.0.nupkg;
        $(TarballPrebuiltPackageDir)nuget.versioning.5.7.1-rtm.4.nupkg;
        $(TarballPrebuiltPackageDir)powershell.7.0.0.nupkg;
