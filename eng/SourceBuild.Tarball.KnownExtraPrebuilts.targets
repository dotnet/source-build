        $(TarballPrebuiltPackageDir)microsoft.codeanalysis.csharp.2.9.0.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.codeanalysis.fxcopanalyzers.2.9.8.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.codeanalysis.versioncheckanalyzer.2.9.8.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.codequality.analyzers.2.9.8.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.diagnostics.netcore.client.0.2.0-preview.20220.1.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.diasymreader.pdb2pdb.*.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.dotnet.arcade.sdk.*.nupkg;
        $(TarballPrebuiltPackageDir)microsoft.dotnet.genapi.*.nupkg;
