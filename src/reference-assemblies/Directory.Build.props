    <Import Project="../../dir.props" />
    <PropertyGroup>
        <OutputPath>$(IntermediatePath)$(MSBuildProjectDirectoryNoRoot.Substring($(MSBuildThisFileDirectoryNoRoot.LastIndexOf("reference-assemblies"))))</OutputPath>
